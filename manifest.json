{
    "manifest_version": 2,
    "name": "Target ATC Extension",
    "version": "1.0",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
    ],
    "browser_action": {
    "default_icon": "target.png",
    "default_popup": "popup.html"
    }
}